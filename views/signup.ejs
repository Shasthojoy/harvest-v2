    <!-- Page Content -->
   <section id="sign-up" class="bg-light-grey">
       <div class="container">
           <div class="row">
               <div class="col-md-7">
                   <h1>Big Heading About Harvest API</h1>
                   <p class="lead">Lorem ipsum dolor sit amet, consectetur adipiscing elit.
                       Vivamus sed mauris non dolor vehicula semper a ornare
                       erat. Suspendisse laoreet fermentum sapien, vel suscipit leo
                       pharetra nec.</p>
                   <img id="signup-pic" src="http://www.placehold.it/500x400">
               </div>
               <div class="col-md-5">
                   <div class="form-wrap">
                       <h2 class="form-head text-center">Create Your Harvest API Account</h2>
                       <form>
                          <div id="name-field" class="row">
                              <div class="col-md-6">
                                  <div class="form-group">
                                      <label for="input1">First Name</label>
                                      <input type="email" class="form-control" id="input1" placeholder="First Name">
                                  </div>
                              </div>
                              <div class="col-md-6">
                                  <div class="form-group">
                                      <label for="input2">Last Name</label>
                                      <input type="email" class="form-control" id="input2" placeholder="Last Name">
                                  </div>
                              </div>
                          </div>
                           <div class="form-group">
                               <label for="input3">Email Address</label>
                               <input type="email" class="form-control" id="input3" placeholder="Email Address">
                           </div>
                           <div class="form-group">
                               <label for="input4">Password</label>
                               <input type="password" class="form-control" id="input4" placeholder="Password">
                           </div>
                           <div class="form-group">
                               <label for="input5">Confirm Password</label>
                               <input type="email" class="form-control" id="input5" placeholder="Confirm Password">
                           </div>
                           <div class="form-group">
                               <label for="input6">Phone Number</label>
                               <input type="password" class="form-control" id="input6" placeholder="Phone Number">
                           </div>
                           <div class="form-group">
                               <label for="input7">Address</label>
                               <input type="email" class="form-control" id="input7" placeholder="Address">
                           </div>
                           <div class="form-group">
                               <label for="input8">Company</label>
                               <input type="password" class="form-control" id="input8" placeholder="Company">
                           </div>
                           <div class="form-group">
                               <label>Intended Use of APIs</label>
                               <!--<input type="email" class="form-control" id="input9" placeholder="Intended Use of APIs">-->
                               <textarea class="form-control" rows="3"></textarea>
                           </div>
                           <div class="checkbox">
                               <label>
                                   <input type="checkbox"> I agree to Harvest API’s <a href="#">Terms of Service</a> and
                                   <a href="#">Privacy Policy</a>.
                               </label>
                           </div>
                           <!--<button type="submit" class="btn btn-default btn-lg drk-grey-btn">Submit</button>-->
                            <a href="confirmation" class="btn btn-default btn-lg drk-grey-btn">Submit</a>
                       </form>

                   </div>
               </div>
